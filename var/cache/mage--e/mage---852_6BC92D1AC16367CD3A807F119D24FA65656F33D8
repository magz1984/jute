
<script src="http://localhost/projects/magento/jute/skin/frontend/Kartparadigm/default/js/jquery.min-1.11.1.js"></script>  
<script type="text/javascript">
/* toTop Button start*/
        var jqTop=$.noConflict();
        jqTop(document).ready(function(){
          jqTop(function() {
                jqTop(window).scroll(function() {
                if(jqTop(this).scrollTop() > 300) {
                    jqTop('#toTop').fadeIn();   
                } else {
                    jqTop('#toTop').fadeOut();
                }
            });
               jqTop('#toTop').click(function() {
                jqTop('body,html').animate({scrollTop:0},500);
            });   
            });
          });
          /* toTop Button end*/
</script>

 
 
 
 
